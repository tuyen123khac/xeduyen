<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/account_nav_graph"
    app:startDestination="@id/signInFragment">

    <fragment
        android:id="@+id/signInFragment"
        android:name="com.tuyenvo.xeduyen.account.fragments.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.tuyenvo.xeduyen.account.fragments.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_signUpSuccessfullyFragment"
            app:destination="@id/signUpSuccessfullyFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpSuccessfullyFragment"
        android:name="com.tuyenvo.xeduyen.account.fragments.SignUpSuccessfullyFragment"
        android:label="fragment_sign_up_successfully"
        tools:layout="@layout/fragment_sign_up_successfully" >
        <action
            android:id="@+id/action_signUpSuccessfullyFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
</navigation>